import io.github.bonigarcia.wdm.WebDriverManager;

public class Test2 {
	public static void main(String[] args) throws InterruptedException {
		WebDriverManager.chromedriver().setup();
		WebDriver d= new ChromeDriver();
		d.get("http://www.uitestpractice.com/");
		d.manage().window().maximize();
		d.manage().timeouts().implicitlyWait(100,TimeUnit.SECONDS);
		d.findElement(By.xpath("//a[normalize-space()='Switch to']")).click();
		d.findElement(By.id("alert")).click();
		Alert alertC = d.switchTo().alert();
		alertC.accept();
		Thread.sleep(2000);
		d.findElement(By.id("confirm")).click();
		Alert alertD = d.switchTo().alert();
		alertD.accept();
		Thread.sleep(2000);
		d.findElement(By.id("prompt")).clear();
		Alert alertE = d.switchTo().alert();
		alertE.accept();
		Thread.sleep(2000);
		d.findElement(By.xpath("//button[normalize-space()='Launch modal']")).click();
		Alert alertF = d.switchTo().alert();
		alertF.accept();
		Thread.sleep(2000);
		d.findElement(By.xpath("//button[@id='basicAuthentication']")).click();
		Alert alertG = d.switchTo().alert();
		alertG.accept();
		Thread.sleep(2000);
		
