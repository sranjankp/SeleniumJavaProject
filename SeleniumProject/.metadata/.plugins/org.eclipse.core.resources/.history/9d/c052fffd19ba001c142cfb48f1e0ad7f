import java.util.Scanner;

public class Io1 {

	public static void main(String[] args) {
		System.out.println("enter the value of a");
		int a;
	Scanner s= new Scanner(System.in);
	a=s.nextInt();

	int b=(100/a);
	try{
		
		if (a==0)
		{
			System.out.println("worng input");
		}
	}
	catch(Exception e)
	{
		System.out.println(b);
	}
	}

}
