import java.util.LinkedList;

public class LinkedList1 {

	public static void main(String[] args) {
		LinkedList s= new LinkedList();
		s.add("shashi");
		s.add("Pruvi");
		s.add("neha");
		System.out.println(s);
        s.addFirst("ranjana");
        System.out.println(s);
        s.addLast("Puja");
        System.out.println(s);
        s.add(0,"silpi");
        System.out.println(s);

	}

}
